<?xml version="1.0" ?>
<robot name="rakuda" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!-- Variabile testa anche inseribile in file launch-->
<xacro:arg name="head_type" default="apollo"/>

<xacro:property name="head_type" value="$(arg head_type)"/>


<xacro:include filename="$(find rakuda_description)/urdf/gemini_335_336.urdf.xacro" />
<xacro:include filename="$(find rakuda_description)/urdf/materials.xacro" />
<xacro:include filename="$(find rakuda_description)/urdf/rakuda.ros2_control.xacro" />
<xacro:include filename="$(find rakuda_description)/urdf/rakuda.gazebo" />

<!-- Teste disponibili -->
<xacro:include filename="$(find rakuda_description)/urdf/head_apollo.urdf.xacro"/>
<xacro:include filename="$(find rakuda_description)/urdf/head_sciurus.urdf.xacro"/>


<!-- Scelta della testa da caricare -->
<xacro:if value="${head_type == 'apollo'}">
  <xacro:head_apollo parent_link="neck_yaw_link"/>
</xacro:if>

<xacro:if value="${head_type == 'sciurus'}">
  <xacro:head_sciurus parent_link="neck_yaw_link"/>
</xacro:if>


<!-- 'base_link'  to Gazebo 'world' 
<link name="world"/>

<joint name="base_link_to_world_joint" type="fixed">
  <parent link="world"/>
  <child link="base_link"/>
</joint>
-->

<!-- Main links -->

<link name="base_link">
