<!-- Joints -->

<joint name="base_waist_joint" type="fixed">
  <origin xyz="0.0 0.0 0.306" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="waist_link"/>
</joint>

<joint name="waist_yaw_joint" type="revolute">
  <origin xyz="0.0 0.0 0.0065" rpy="0 0 0"/>
  <parent link="waist_link"/>
  <child link="torso_link"/>
  <axis xyz="0.0 0.0 1.0"/>
  <limit upper="1.396263" lower="-1.396263" effort="100" velocity="100"/>
  <dynamics damping="1.0e-6" friction="0.8"/>
</joint>

